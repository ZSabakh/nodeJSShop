<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (items.length > 0) { %>
                <ul>
                    <% items.forEach(item => { %>
                        <li>
                            <%= item.itemInfo.title %> - <%= item.quantity %> qty </li> 
                        </li>
                    <% }) %>

                    
                </ul>
            <% } else { %>
                <h1>Nothing in cart</h1>
                <% } %>

        </main>
        <%- include('../includes/end.ejs') %>